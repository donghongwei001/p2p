<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.zkjdao">	
	<insert paramType="com.entity.Zkjproject">
		insert into projectapplication(ID,userid,projectname,projecttype,location,time,money,lifeloan,ratemoney)values(projectapplicationid.nextval,#{userid},#{projectname},#{projecttype},#{location},#{time}#{money},#{lifeloan},#{ratemoney})
	</insert>
	
</mapper>