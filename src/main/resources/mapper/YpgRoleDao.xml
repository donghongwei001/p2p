<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.YpgRoleDao">	
	<select id="queryRole" resultType="java.util.HashMap">
		select * from role
	</select>
	<select id="" resultType="java.util.HashMap">
		select roleid from role where rolename=#{rolename}
	</select>
</mapper>