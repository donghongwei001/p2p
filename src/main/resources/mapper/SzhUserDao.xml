<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.SzhUserDao">	
	<select id="queryUser" resultType="com.entity.SzhUser">
		select * from usertable
	</select>
	<insert id="addUser" parameterType="com.entity.SzhUser">
		insert into usertable values(#{userID},#{username},#{pwd},#{money})
	</insert>
</mapper>