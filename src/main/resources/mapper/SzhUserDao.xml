<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.SzhUserDao">	
	<select id="queryUser" resultType="com.entity.SzhUser">
		select * from usertable
	</select>
	<insert id="addUser" parameterType="com.entity.SzhUser">
		insert into usertable values(#{userID,jdbcType=VARCHAR},#{username,jdbcType=VARCHAR},#{pwd,jdbcType=VARCHAR},#{money,jdbcType=VARCHAR})
	</insert>
	<select id="queryRole" resultType="com.entity.SzhRole">
		select * from role
	</select>
	<insert id="addRole" parameterType="com.entity.SzhRole">
		insert into role values(#{roleid,jdbcType=VARCHAR},#{rolename,jdbcType=VARCHAR},#{rolestatus,jdbcType=VARCHAR})
	</insert>
</mapper>