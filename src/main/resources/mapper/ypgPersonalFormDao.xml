<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.ypgPersonalFormDao">	
	<insert id="insertPersonalForm" parameterType="com.entity.ypgPersonalForm">
		insert into psersonalform values(psersonalid.nextval,#{userID,jdbcType=NUMERIC},#{personalname},#{idcard},#{iphone},#{accountnumber},#{mailbox},#{sex},to_date(#{birthday},"yyyy-MM-dd"),#{educational},#{marriage},#{havachidren},#{address},#{homephone},#{worklife},#{occupation},#{workunit},#{unitproperties},#{unitphone},#{monthlyincome},#{haveroom},#{houseloan},#{havecar},#{carloan},#{contactname},#{contactrelation},#{contactphone})
	</insert>
	
</mapper>